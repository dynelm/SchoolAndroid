<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.dynelm.robotdarttest.MainActivity"
    android:background="@drawable/dart_n">


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/line"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="49dp" />

    <ImageView
        android:id="@+id/Img_Home_page"
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/zhuye_n"
        android:layout_marginStart="46dp"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_alignParentStart="true" />

    <ImageView
        android:id="@+id/Img_Nav"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="40dp"
        app:srcCompat="@drawable/daohang_n"
        android:layout_above="@+id/textView2"
        android:layout_toEndOf="@+id/Img_Home_page" />

    <ImageView
        android:id="@+id/Img_FaceRec"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="36dp"
        app:srcCompat="@drawable/face"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_toEndOf="@+id/Img_Nav" />

    <ImageView
        android:id="@+id/Img_Video_New"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="47dp"
        app:srcCompat="@drawable/shiping_n"
        android:layout_alignTop="@+id/Img_FaceRec"
        android:layout_toEndOf="@+id/Img_FaceRec" />

    <ImageView
        android:id="@+id/Img_Contact"
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:srcCompat="@drawable/coustom_n"
        android:layout_marginStart="26dp"
        android:layout_alignBottom="@+id/imageView3"
        android:layout_toEndOf="@+id/Img_Video_New" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="主页"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:layout_toStartOf="@+id/Img_Nav"
        android:layout_marginEnd="16dp"
        android:layout_below="@+id/Img_Home_page" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="导航"
        android:textSize="20sp"
        android:textColor="#ffffff"
        android:layout_alignBaseline="@+id/textView"
        android:layout_alignBottom="@+id/textView"
        android:layout_toStartOf="@+id/Img_FaceRec"
        android:layout_marginEnd="17dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView2"
        android:layout_toStartOf="@+id/Img_Video_New"
        android:text="人脸识别"
        android:textSize="20sp"
        android:textColor="#ffffff"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignEnd="@+id/Img_Video_New"
        android:text="视频监控"
        android:textColor="#ffffff"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/Img_Contact"
        android:layout_below="@+id/Img_Home_page"
        android:text="排队取号"
        android:textColor="#ffffff"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="200dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/robot_n"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/imageView6" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="500dp"
        android:layout_height="600dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="68dp"
        android:layout_toEndOf="@+id/imageView3"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/chat_n" />

    <FrameLayout
        android:id="@+id/Fragement_layout"
        android:layout_width="850dp"
        android:layout_height="540dp"
        android:layout_marginBottom="45dp"
        android:layout_above="@+id/Img_Home_page"
        android:layout_alignParentStart="true">

    </FrameLayout>

    <ListView
        android:id="@+id/New_ListView"
        android:layout_width="420dp"
        android:layout_height="420dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/imageView6"
        android:layout_alignEnd="@+id/imageView6"
        android:layout_alignBottom="@+id/imageView6" />

    <Button
        android:id="@+id/New_Btn_Start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView5"
        android:layout_toEndOf="@+id/imageView3"
        android:text="开始" />

    <Button
        android:id="@+id/New_Btn_stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="停止"
        android:layout_alignBaseline="@+id/New_Btn_Start"
        android:layout_alignBottom="@+id/New_Btn_Start"
        android:layout_toEndOf="@+id/New_Btn_Start"
        android:layout_marginStart="61dp" />

    <TextView
        android:id="@+id/Time_What"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="15dp"
        android:text="16.21周二"
        android:textColor="#ffffff"
        android:textSize="30sp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/New_ListView" />

    <WebView
        android:id="@+id/webView"
        android:layout_width="800dp"
        android:layout_height="500dp"
        android:layout_above="@+id/imageView5"
        android:layout_alignEnd="@+id/Fragement_layout">

        <Button
            android:id="@+id/btn_close_Web"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="708dp"
            android:layout_y="450dp"
            android:text="Button" />
    </WebView>

</RelativeLayout>
